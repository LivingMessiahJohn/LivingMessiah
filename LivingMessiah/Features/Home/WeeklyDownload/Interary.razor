
<div class="card-body ">
	@* <div class="card border border-1 border-dark "> *@
	<div class="card bg-secondary-subtle">
		<div class="card-header fs-3">Itinerary</div>
		<div class="card-body">
			<p class="fs-5 text-center text-danger mb-0">
				Sabbath worship service begins at ...
			</p>

			<p class="fs-1 text-center text-danger mt-0 mb-1">
				<b>3:00 pm</b>
				<sup class="fs-6 text-black-50">
					<a href="https://www.timeanddate.com/time/zone/usa/arizona" target="_blank" title="Arizona Time Zone">
						AZ TZ
					</a>
				</sup>
			</p>

		</div>
		<div class="card-body text-center mt-0">

			<span class="fs-5 text-center">
				<i class="@Nav.Liturgy.Icon"></i>
				@Enums.Nav.Liturgy.Title
				<a class="btn btn-primary btn-xs"
					 href="@Enums.Nav.Liturgy.Index" title="Go to the @Enums.Nav.Liturgy.Title wizard">
					<i class="fas fa-chevron-right"></i>
				</a>
			</span>
		</div>
	</div>
</div>

